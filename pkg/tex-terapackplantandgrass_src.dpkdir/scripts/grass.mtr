
textures/terapack/grass/grass_1
{
    qer_editorimage textures/terapack/grass/pattern_42_diffuse.tga
    {
        map textures/terapack/grass/pattern_42_diffuse.tga
    }
    {
        stage normalparallaxmap
        map textures/terapack/grass/pattern_42_normal.tga
        normalScale 1 1
        parallaxDepth 0.05
    }
    {
        stage specularmap
        map textures/terapack/grass/pattern_42_specular.tga
        specularReflectance 0.4
        specularExponent 256
    }
    {
        map $lightmap
        blendfunc GL_DST_COLOR GL_ZERO
    }
}

